<keymap version="1" name="GNOME Proper Redo" parent="Default for GNOME">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="ctrl back_slash" second-keystroke="d" />
    <keyboard-shortcut first-keystroke="alt k" />
  </action>
  <action id="Git.CompareWithBranch">
    <keyboard-shortcut first-keystroke="ctrl back_slash" second-keystroke="c" />
    <keyboard-shortcut first-keystroke="alt g" second-keystroke="c" />
  </action>
  <action id="IntroduceVariable">
    <keyboard-shortcut first-keystroke="ctrl alt v" />
    <keyboard-shortcut first-keystroke="alt v" />
  </action>
  <action id="ReopenClosedTab">
    <keyboard-shortcut first-keystroke="ctrl back_slash" second-keystroke="t" />
  </action>
  <action id="Scala.ShowImplicits">
    <keyboard-shortcut first-keystroke="shift ctrl alt equals" />
    <keyboard-shortcut first-keystroke="shift ctrl alt add" />
  </action>
  <action id="TogglePresentationMode">
    <keyboard-shortcut first-keystroke="ctrl back_slash" second-keystroke="p" />
  </action>
  <action id="ToggleZenMode">
    <keyboard-shortcut first-keystroke="ctrl back_slash" second-keystroke="z" />
  </action>
</keymap>